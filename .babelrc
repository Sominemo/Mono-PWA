{
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-syntax-dynamic-import",
        "@babel/transform-runtime",
        "@babel/plugin-transform-strict-mode"
    ],
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": ">5% in UA",
                "useBuiltIns": "usage",
                "corejs": {
                    "version": 3,
                    "proposals": true
                }
            }
        ]
    ]
}
